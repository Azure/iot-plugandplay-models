          "enumValue": 0,
          "name": "Off"
        },
        {
          "@id": "dtmi:com:Microchip:WFI32_IoT_WM:LedState:On;1",
          "displayName": {
            "en": "LED On"
          },
          "enumValue": 1,
          "name": "On"
        },
        {
          "@id": "dtmi:com:Microchip:WFI32_IoT_WM:LedState:Blink;1",
